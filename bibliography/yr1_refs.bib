@article{Unold,
abstract = {The influence of composition on the quaternary Cu2ZnSnS4 (CZTS) absorber material on the secondary phase content, recombination characteristics and solar cell performance is investigated. Best solar efficiencies are found in a very narrow Cu-poor, Zn-rich composition range. Photoluminescence measurements performed at 12 K indicate strong compensation for Cu-poor composition, similar to what is observed in chalcopyrite-type compounds. We find that strongly copper-deficient CZTS contains significant amounts of ZnS, which degrades the solar cell performance. },
author = {Unold, Thomas and Kretzschmar, Steffen and Just, Justus and Zander, Ole and Schubert, Bjorn and Marsen, Bjorn and Schock, Hans-Werner;},
