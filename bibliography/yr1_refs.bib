number = {22},
pages = {2013},
title = {{Luminescence of $Cu_{2}ZnSnS_{4}$ polycrystals described by the fluctuating potential model}},
url = {file:///C:/Users/poguedg/Downloads/JAP{\_}113{\_}223503{\_}2013{\_}potential fluct (2).pdf},
