journal = {Sol. Energy},
keywords = {CZTS,Defect transition levels,First-principles calculations,Na doping},
mendeley-groups = {MRes\_CZTS/Google Scholar Alerts},
pages = {125--132},
publisher = {Elsevier Ltd},
title = {{Intrinsic defects and Na doping in Cu2ZnSnS4: A density-functional theory study}},
