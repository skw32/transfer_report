issn = {00218979},
journal = {J. Appl. Phys.},
mendeley-groups = {PhD{\_}year1/Projects/V{\_}S/V{\_}Se paper (discussions with Pepe)},
number = {12},
title = {{Model of native point defect equilibrium in Cu2ZnSnS4 and application to one-zone annealing}},
volume = {114},
