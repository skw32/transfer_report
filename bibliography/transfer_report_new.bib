title = {{Research Update: Relativistic origin of slow electron-hole recombination in hybrid halide perovskite solar cells}},
url = {http://scitation.aip.org/content/aip/journal/aplmater/4/9/10.1063/1.4955028},
volume = {4},
year = {2016}
}


